.a_banner{
  height:260px;

text-align:center;
}
.a_banner image{
  width: 670rpx;
  height: 260px;
}
.a_show{
  height:120px;
  
overflow-x:auto;

}
.a_show .a_show_view{
  overflow:hidden;
  width:200px;
  height:100px;
  float:left;
  padding-top:20px;
  display: inline-block;
}
.a_show .a_show_view:nth-child(1){
  margin-left: 40rpx;
}
.a_show_exchange{
  width:110px;float:left;height:90px;margin-left:8px;
}
.a_show_exchange>text:nth-child(1){
  display:block;font-weight:bold;
  height:20px;
overflow:hidden;
white-space:nowrap;
text-overflow:ellipsis;
font-size:32rpx;
margin-bottom:10rpx;


}
.a_show_exchange>text:nth-child(2){
  font-size:14px;line-height:20px;height:60px;display:block;overflow:hidden;
}
.a_attend{
  height: 80px;
  padding:0px 30px 0 30px;

}
.a_attend view{
  text-align:center;
height:54px;
line-height:54px;
border-radius:10px;
background:#cf3928;
color:#fff;
}
.a_bottom{
  /* padding:0 20px; */
  width: 670rpx;
  /* height: 330rpx; */
  margin:0 auto;
padding-top:54rpx;

  background: url(data:image/png;base64,);
  margin:0 auto;
background-size:100%;
padding-bottom:20px;
}
.cj_btn{
  width:236rpx;height:60rpx;text-align:center;line-height:30px;margin:0px auto 10px;border-radius:10px;background:#cf3928;color:#fff;
}
.madol{
  position: fixed;
  left: 0;
  top:0;
  width: 100%;
  height: 100%;
  z-index: 1000;
}
.mask{
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,.3);

}
.madol_box{
  width: 230px;
  /* height: 290px; */
  padding:20px 40rpx 30px;
  position: absolute;
  left: 50%;
  top: 20%;
  transform: translateX(-50%);
  background:#f1f2f3;
border-radius:10rpx;
}
.madol_click>view{
/* display:inline-block; */
/* width:80px; */
height:40px;
text-align:center;
line-height:40px;
background:#cf3928;
border-radius:20rpx;
color:#fff;

}