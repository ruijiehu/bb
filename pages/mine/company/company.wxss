.weui-uploader__input-box:before{
  content: unset;
}
.weui-uploader__input-box:after{
  content: '上传';
  border: 0;
  background: unset;
  height: 40rpx;
  top:50%;
left:50%;
-webkit-transform:translate(-50%, -50%);
transform:translate(-50%, -50%);
color: #D9D9D9;
}
.weui-uploader__input-box:active:after{
 background: unset;
 color: #999999;
}