/* 绑定手机弹框 */
.layer-bind-phone{
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: 999999;
  left: 0;
  top: 0;
  background: rgba(0, 0, 0,0.3);
}
.layer-dialog{
  width: 540rpx;
  height: 350rpx;
  padding: 30rpx 20rpx 20rpx;
  position: absolute;
  top:50%;
  left: 50%;
  margin-top: -175rpx;
  margin-left: -270rpx;
  background: #fff;
  border-radius: 10rpx;
}
.layer-close{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 0;
  top: 0;
}
.layer-close image{
  width:36rpx;
  height: 36rpx;
  margin-top: 10rpx;
  margin-left: 10rpx;
}
.layer-title{
  text-align: center;
  font-size: 16rpx;
}
.layer-title image{
  width: 200rpx;
  height: 87rpx;
  display:block;
margin:0 auto;

}
.layer-content input{
 display: inline-flex;
 width: 340rpx;
 /* padding: 12rpx 0 4rpx 0; */
 padding-left: 20rpx;
 vertical-align: middle;
}
